{
  "name": "re-active-react",
  "sourceRoot": "packages/re-active/react/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/re-active/react/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nrwl/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/re-active/react",
        "tsConfig": "packages/re-active/react/tsconfig.lib.json",
        "project": "packages/re-active/react/package.json",
        "entryFile": "packages/re-active/react/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/re-active/react/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    }
  }
}
